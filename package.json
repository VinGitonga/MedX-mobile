    "@react-navigation/native-stack": "^6.4.0",
    "expo": "~44.0.0",
    "expo-linear-gradient": "~11.0.1",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-calendars": "^1.1277.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "tailwind-react-native-classnames": "^1.5.1",
    "tailwind-rn": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
